#include <bits/stdc++.h>

using namespace std;

int main(){
    freopen("in.txt","r",stdin);
    freopen("out.txt","w",stdout);
    int n;
    while(cin>>n && n!=0){
        vector<int>a(n);
        vector<int>::iterator it = a.begin();
        for(auto &x: a) cin>>x;
        sort(a.begin(),a.end());
        for(it = a.begin(); it+1 != a.end(); it++){
            cout<<*it<<" ";
        }

        cout<<*it<<endl;

    }
    return 0;
}
